{
  "name": "vue2-to-3",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint ./**/*.{ts,js}",
    "lint:fix": "eslint ./**/*.{ts,js} --fix",
    "prettier": "prettier --config ./.prettierrc --write ./**/*.ts",
    "start:parser": "node --loader ts-node/esm --experimental-specifier-resolution=node parser/parser",
    "start:file": "node --loader ts-node/esm --experimental-specifier-resolution=node fileCreator/index",
    "do": "node --loader ts-node/esm --experimental-specifier-resolution=node index"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vue-template-compiler": "^2.6.11"
  },
  "bin": {
    "migrate": "npm run do"
  }
}
